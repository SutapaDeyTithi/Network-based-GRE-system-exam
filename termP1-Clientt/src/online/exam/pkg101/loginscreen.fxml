<AnchorPane id="AnchorPane" prefHeight="707.0" prefWidth="978.0" style="-fx-background-color: #CCCCFF;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="online.exam.pkg101.LoginscreenController">
